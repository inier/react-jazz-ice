    "react-activation": "^0.9.7",
    "react-dom": "^17.0.2",
    "react-iframe": "^1.8.0",
    "react-intl": "^5.24.0",
    "react-router-dom": "^5.0.0",
    "react-sortablejs": "^6.0.0",
    "sortablejs": "^1.14.0",
    "store2": "^2.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@iceworks/spec": "^1.5.0",
    "@ozo/mixins-sass": "^1.1.11",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "assert": "^2.0.0",
    "babel-plugin-import": "^1.13.3",
    "body-parser": "^1.19.1",
    "build-plugin-css-assets-local": "^0.1.0",
    "build-plugin-dev-inspector": "^0.1.2",
    "build-plugin-fusion": "^0.1.22",
    "build-plugin-keep-alive": "^1.8.1",
    "build-plugin-moment-locales": "^0.1.3",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "conventional-changelog": "^3.1.25",
    "env-cmd": "^10.1.0",
    "eslint": "^7.30.0",
    "express": "^4.17.2",
    "express-useragent": "^1.0.15",
    "gh-pages": "^3.2.3",
    "husky": "^4.2.3",
    "ice.js": "^2.4.3",
    "jest": "^27.4.5",
    "lint-md": "^0.2.0",
    "lint-md-cli": "^0.1.2",
    "lint-staged": "^12.1.5",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "react-test-renderer": "^17.0.2",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.2.0"
  },
  "scripts": {
    "dev": "icejs start --disable-mock --config build.config.js --mode local --port 8889",
    "start": "icejs start --config build.config.js --mode local --port 8888",
    "build": "icejs build --config build.config.js --mode prod",
    "debug": "icejs build --config build.config.js --mode debug",
    "analyze": "source-map-explorer build/js/main*.*",
    "test": "icejs test --config build.config.js",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint --cache --format table --ext .js,.jsx,.ts,.tsx ./src",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint **/*.{css,scss,less} --cache",
    "stylelint:fix": "npm run stylelint --fix",
    "mdlint": "lint-md **/*.{md,markdown}",
    "mdlint:fix": "lint-md **/*.{md,markdown} --fix",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier -c --write \"src/**/*.{js,jsx,ts,tsx,css,scss,less,json}\" --end-of-line auto",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "autoFormat": "onchange src/**/*.js src/**/*.jsx src/**/*.scss -- prettier --write {{changed}}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'Pre-commit checks......' && lint-staged --allow-empty",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scaffoldConfig": {
    "name": "Ice Admin - TS",
    "title": "Ice Admin - TS",
    "category": "Basic",
    "screenshot": "https://img.alicdn.com/tfs/TB1jzt1YEz1gK0jSZLeXXb9kVXa-1349-833.png"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inier/react-jazz-ice.git"
  },
  "title": "react-admin-ts基础模板"
}
