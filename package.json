    "screenshot.png",
    "tsconfig.json",
    "jsconfig.json"
  ],
  "dependencies": {
    "@alifd/next": "^1.25.11",
    "@alifd/theme-design-pro": "0.x",
    "@antv/data-set": "^0.11.8",
    "@ice/stark": "^2.7.1",
    "@ozo/lazy-loader": "^1.0.5",
    "ahooks": "^3.1.4",
    "axios": "^0.24.0",
    "axios-extensions": "^3.1.3",
    "bizcharts": "^4.1.15",
    "classnames": "^2.3.1",
    "crypto-js": "^4.1.1",
    "lodash-es": "^4.17.21",
    "lru-cache": "^6.0.0",
    "mobx": "^6.3.10",
