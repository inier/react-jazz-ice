{
  "name": "@ozo/ice-admin-ts",
  "version": "0.0.1",
  "description": "基于ice.js、TypeScript的管理端模板工程。",
  "files": [
    ".vscode/",
    "src/",
    "build/",
    "public/",
    "tests/",
    "_gitignore",
    "_editorconfig",
    "_eslintignore",
    "_eslintrc.js",
    "_prettierignore",
    "_prettierrc.js",
    "_stylelintignore",
    "_stylelintrc.js",
    "build.json",
    "screenshot.png",
    "tsconfig.json",
    "jsconfig.json"
  ],
  "dependencies": {
    "@alifd/next": "^1.21.0",
    "@alifd/theme-design-pro": "0.x",
    "@antv/data-set": "^0.10.2",
    "@ozo/lazy-loader": "^1.0.5",
    "ahooks": "^2.10.12",
    "bizcharts": "^3.5.6",
    "lodash-es": "^4.17.15",
    "lodash.debounce": "^4.0.8",
    "mobx": "^6.3.6",
    "mobx-react": "^7.2.1",
    "moment": "^2.24.0",
    "prop-types": "^15.5.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^2.8.0",
    "react-sortablejs": "^5.0.0",
    "sortablejs": "^1.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@iceworks/spec": "^1.0.0",
    "@ozo/mixins-sass": "^1.1.7",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "assert": "^2.0.0",
    "babel-plugin-import": "^1.13.0",
    "body-parser": "^1.19.0",
    "build-plugin-css-assets-local": "^0.1.0",
    "build-plugin-dev-inspector": "^0.1.1",
    "build-plugin-fusion": "^0.1.0",
    "build-plugin-keep-alive": "^1.8.1",
    "build-plugin-moment-locales": "^0.1.0",
    "chalk": "^3.0.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "conventional-changelog": "^3.1.18",
    "env-cmd": "^10.1.0",
    "eslint": "^7.30.0",
    "express-useragent": "^1.0.13",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.3",
    "ice.js": "^2.0.0",
    "jest": "^27.3.1",
    "lint-md": "^0.2.0",
    "lint-md-cli": "^0.1.2",
    "lint-staged": "^10.0.9",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.1",
    "stylelint": "^13.2.0"
  },
  "scripts": {
    "start": "icejs start --config build.config.js --mode local",
    "build": "icejs build --config build.config.js --mode prod",
    "debug": "icejs build --config build.config.js --mode debug",
    "prepublishOnly": "npm run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "icejs test --config build.config.js",
    "lint": "npm run eslint && npm run stylelint && npm run lint:prettier",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "eslint": "eslint --cache src/**/*.{js,jsx,ts,tsx} --format table",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint src/**/*.{css,scss,less} --cache",
    "stylelint:fix": "stylelint src/**/*.{css,less,scss} --cache --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,sass,scss,less,json,md,mdx}",
    "mdlint": "lint-md **/*.{md,markdown}",
    "mdlint:fix": "lint-md **/*.{md,markdown} --fix",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "autoFormat": "onchange src/**/*.js src/**/*.jsx src/**/*.scss -- prettier --write {{changed}}",
    "analyze": "icejs build && source-map-explorer build/static/js/main.*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'Pre-commit checks......' && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scaffoldConfig": {
    "name": "Ice Admin - TS",
    "title": "Ice Admin - TS",
    "category": "Basic",
    "screenshot": "https://img.alicdn.com/tfs/TB1jzt1YEz1gK0jSZLeXXb9kVXa-1349-833.png"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inier/react-jazz-ice.git"
  },
  "title": "react-admin-ts基础模板"
}
