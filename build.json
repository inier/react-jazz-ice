{
  "publicPath": "./",
  "define": {
    "PUBLIC_URL": "/toxic"
  },
  "vite": true,
  "externals": {
    "@alifd/next": "Next"
  },
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": "@alifd/theme-design-pro",
        "themeConfig": {
          "icon-font-path": "'/icon-font/icon'",
          "font-custom-path": "'/font/'"
        },
        "externalNext": true
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ],
    [
      "build-plugin-css-assets-local",
      {
        "outputPath": "assets",
        "relativeCssPath": "../"
      }
    ],
    [
      "build-plugin-keep-alive"
    ],
    [
      "build-plugin-dev-inspector"
    ]
  ],
  "proxy": {
    "/api": {
      "enable": true,
      "target": "http://127.0.0.1:6666"
    }
  }
}